{
  "name": "vanilla-carousel",
  "version": "1.1.1",
  "description": "A simple easy to use plain vanilla carousel.",
  "keywords": [
    "carousel",
    "vanilla"
  ],
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "main": "./dist/carousel",
  "scripts": {
    "demo-clean": "rimraf ./public",
    "demo-build": "npm run demo-clean && parcel build demo/index.html --out-dir ./public --cache-dir ./.cache",
    "demo-start": "parcel demo/index.html --open --out-dir ./public --cache-dir ./.cache",
    "demo": "run-s demo-clean demo-start",
    "build": "gulp",
    "start": "gulp watch"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/node": "^11.13.8",
    "gulp": "^4.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-minify": "^3.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.6.1",
    "rimraf": "^2.6.3",
    "sass": "^1.19.0",
    "typescript": "^3.4.5"
  }
}
